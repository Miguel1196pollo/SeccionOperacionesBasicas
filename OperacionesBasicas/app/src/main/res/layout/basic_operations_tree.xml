        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="1">



        <ImageButton
            android:layout_width="100dp"
            android:layout_height="80dp"
            android:text="@string/numero_34"
            android:padding="20dp"
            android:src="@drawable/numero34"
            android:id="@+id/ib_numero_34"
            android:layout_margin="1dp"
            />

        <ImageButton
            android:layout_width="100dp"
            android:layout_height="80dp"
            android:text="@string/numero_35"
            android:padding="20dp"
            android:src="@drawable/numero35"
            android:id="@+id/ib_numero_35"
            android:layout_margin="1dp"
            android:layout_marginRight="3dp"
            />

        <ImageButton
            android:layout_width="100dp"
            android:layout_height="80dp"
            android:text="@string/numero_36"
            android:padding="20dp"
